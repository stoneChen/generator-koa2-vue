{
  "name": "generator-koa2-vue",
  "version": "1.2.0",
  "description": "used to scaffolding koa2 + vuejs projects",
  "main": "app/index.js",
  "files": [
    "app"
  ],
  "scripts": {
    "vp": "mversion patch -m 'Bumped to v%s'",
    "vm": "mversion minor -m 'Bumped to v%s'",
    "vmajor": "mversion major -m 'Bumped to v%s'",
    "pub-minor": "npm run vm && npm publish"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "authors": [
    {
      "name": "cloudstone",
      "email": "baby31529@gmail.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.1",
    "yosay": "^0.3.0",
    "yeoman-generator": "^0.21.1"
  },
  "devDependencies": {
    "mversion": "^1.10.1"
  }
}
